{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
          "error",
          2
        ],
        "linebreak-style": [
          "error",
          "windows"
        ],
        "quotes": [
          "error",
          "double"
        ],
        "semi": [
          "error",
          "always"
        ],
        "no-cond-assign": "error",
        "no-extra-parens": 1,
        "no-unsafe-negation": 2,
        "valid-jsdoc": 2,
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "class-methods-use-this": 2,
        "complexity": ["warn", 10],
        "consistent-return": 2,
        "default-case": 2,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-floating-decimal": 2,
        "no-global-assign": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": ["warn", {"ignore": [0, 1]}],
        "no-multi-str": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 2,
        "no-proto": 2,
        "no-return-assign": 2,
        "no-return-await": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 1,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-useless-escape": 2,
        "no-void": 2,
        "no-warning-comments": 1,
        "no-with": 2,
        "require-await": 2,
        "vars-on-top": 2,
        "init-declarations": 1,
        "no-catch-shadow": 2,
        "no-label-var": 2,
        "no-restricted-globals": ["error", "event", "fdescribe"],
        "no-shadow-restricted-names": 2,
        "no-shadow": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-use-before-define": 2,
        "camelcase": 2,
        "consistent-this": ["error", "that"],
        "func-name-matching": 2,
        "func-style": ["error", "declaration"],
        "id-blacklist": ["error", "data", "err", "e", "cb", "callback"],
        "id-length": 2,
        "line-comment-position": ["error", { "position": "above" }],
        "max-depth": 2,
        "max-lines": ["warn", 800],
        "max-nested-callbacks": ["error", 3],
        "max-params": 2,
        "max-statements-per-line": ["error", { "max": 1 }],
        "max-statements": 2,
        "new-cap": 2,
        "newline-per-chained-call": ["error", { "ignoreChainWithDepth": 2 }],
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-continue": 2,
        "no-inline-comments": 2,
        "no-mixed-operators": 2,
        "no-multiple-empty-lines": 2,
        "no-negated-condition": 2,
        "no-new-object": 2,
        "no-plusplus": 2,
        "no-underscore-dangle": 2,
        "no-unneeded-ternary": 2,
        "object-property-newline": 2,
        "one-var": 2,
        "operator-linebreak": 2,
        "require-jsdoc": 2
    }
}
